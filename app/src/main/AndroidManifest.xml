<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.libs.jiaop">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AndPermissionActivity" />
        <activity android:name=".mvp.MvpActivity" />
        <activity android:name=".SecActivity" />
        <activity android:name=".MfActivity" />
        <activity android:name=".SystemBarTintActivity" />
        <activity android:name=".KeyBoardActivity" />
        <activity
            android:name=".KBActivity"
            android:configChanges="orientation|keyboardHidden"
            android:windowSoftInputMode="adjustUnspecified|stateHidden"></activity>
    </application>

</manifest>